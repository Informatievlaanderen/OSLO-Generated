      }
    },
    {
      "@id": "https://data.vlaanderen.be/ns/mobiliteit/trips-en-aanbod#Afstand",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "description": {
        "nl": "Ruimte tussen twee punten, meestal gemeten langs een rechte lijn."
      },
      "name": {
        "nl": "Afstand"
      },
      "parents": [],
      "usage": {}
    },
    {
      "@id": "https://data.vlaanderen.be/ns/mobiliteit/trips-en-aanbod#Beschikbaarheid",
      "@type": "http://www.w3.org/2002/07/owl#Class",
