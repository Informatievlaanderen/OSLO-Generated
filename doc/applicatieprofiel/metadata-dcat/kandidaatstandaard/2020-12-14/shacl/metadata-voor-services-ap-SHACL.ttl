<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#AgentShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdf:langString;
    shacl:description "De naam van de agent";
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "naam";
    shacl:path <http://data.vlaanderen.be/ns/generiek#naam>
  ];
  shacl:targetClass dc:Agent .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusRecordShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class dcat:Resource;
    shacl:description "de geregistreerde catalogusResource";
    shacl:maxCount 1;
    shacl:name "hoofdonderwerp";
    shacl:path foaf:primaryTopic
  ], [
    shacl:datatype rdf:langString;
    shacl:description "de naam van het registratie van de catalogusresource ";
    shacl:maxCount 1;
    shacl:name "titel";
    shacl:path dc:title
  ], [
    shacl:datatype xsd:dateTime;
    shacl:description "Het moment waarop de registratie van de catalogusResource voor de eerste keer gebeurde in de catalogus";
    shacl:maxCount 1;
    shacl:name "aanmaakdatum";
    shacl:path dc:issued
  ], [
    shacl:class <http://www.w3.org/ns/adms#Identifier>;
    shacl:description "een identificator van het catalogusrecord";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "identificator";
    shacl:path dc:identifier
  ];
  shacl:targetClass dcat:CatalogRecord .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusResourceShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class dc:Agent;
    shacl:description "De uitgever van de catalogusResource";
    shacl:maxCount 1;
    shacl:name "uitgever";
    shacl:path dc:publisher
  ];
  shacl:targetClass dcat:Resource .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class dc:Agent;
    shacl:description "de uitgever van een catalogus";
    shacl:maxCount 1;
    shacl:name "uitgever";
    shacl:path dc:publisher
  ], [
    shacl:class dcat:Dataset;
    shacl:description "geregistreerde dataset in de catalogus ";
    shacl:name "heeftDataset";
    shacl:path dcat:dataset
  ], [
    shacl:class <http://www.w3.org/ns/adms#Identifier>;
    shacl:description "een identificator voor de catalogus";
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "identificator";
    shacl:path dc:identifier
  ], [
    shacl:datatype rdf:langString;
    shacl:description "de naam voor de catalogus";
    shacl:maxCount 1;
    shacl:name "titel";
    shacl:path dc:title
  ], [
    shacl:class dc:LicenseDocument;
    shacl:description "de licentie voor de catalogus";
    shacl:maxCount 1;
    shacl:name "licentie";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#licentie>
  ], [
    shacl:class dcat:CatalogRecord;
    shacl:description "provenance informatie over de registratie van een catalogsResource in de catalogus ";
    shacl:name "record";
    shacl:path dcat:record
  ], [
    shacl:datatype rdf:langString;
    shacl:description "een bondige omschrijving van de catalogus";
    shacl:maxCount 1;
    shacl:name "beschrijving";
    shacl:path dc:description
  ], [
    shacl:class vcard:Kind;
    shacl:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de catalogus";
    shacl:maxCount "n"^^xsd:integer;
    shacl:name "contactinformatie";
    shacl:path dcat:contactPoint
  ], [
    shacl:class dcat:DataService;
    shacl:description "geregistreerde DataServices in de catalogus";
    shacl:name "heeftDataService";
    shacl:path dcat:service
  ];
  shacl:targetClass dcat:Catalog .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DataServiceShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:anyURI;
    shacl:description "landingspagina met informatie over de wettelijke beperkingen i.v.m. dataService";
    shacl:maxCount 1;
    shacl:name "wettelijkeBeperkingen";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#wettelijkeBeperkingen>
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "landingspagina met informatie over de genomen voorwaarden om de service te mogen gebruiken. ";
    shacl:maxCount 1;
    shacl:name "gebruiksbepaling";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#gebruiksbepaling>
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "Algemene pagina met vaak beschrijvende tekst";
    shacl:maxCount 1;
    shacl:name "landingspagina";
    shacl:path dcat:landingPage
  ], [
    shacl:class dcat:Dataset;
    shacl:description "data die aangeboden wordt door de datasService";
    shacl:name "biedtInformatieAanOver";
    shacl:path dcat:servesdataset
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "Een beschrijving van de diensten die beschikbaar zijn via de end-points, met inbegrip van hun werking, parameters, enz. ";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "endpointbeschrijving";
    shacl:path dcat:endpointDescription
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "landingspagina met informatie over kwaliteit van de dienstverlening";
    shacl:maxCount 1;
    shacl:name "dienstverleningsKwaliteit";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#dienstverleningsKwaliteit>
  ], [
    shacl:class vcard:Kind;
    shacl:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataservice. ";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "contactinformatie";
    shacl:path dcat:contactPoint
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "De rootlocatie of het primaire eindpunt van de dienst (een web-resolvable URI)";
    shacl:maxCount 1;
    shacl:name "endpointURL";
    shacl:path dcat:endpointURL
  ], [
    shacl:datatype rdf:langString;
    shacl:description "naam voor de dataService. ";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "titel";
    shacl:path dc:title
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Een bondige omschrijving van de dataService";
    shacl:maxCount 1;
    shacl:name "beschrijving";
    shacl:path dc:description
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "landingspagina met informatie over authenticatie";
    shacl:maxCount 1;
    shacl:name "authenticatie";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#authenticatie>
  ], [
    shacl:class <http://www.w3.org/ns/adms#Identifier>;
    shacl:description "Een unieke identificator van de dataService";
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "identificator";
    shacl:path dc:identifier
  ], [
    shacl:datatype xsd:string;
    shacl:description "unieke aanduiding van de gedeployde service toestand gegeven door de endpointbeheerder";
    shacl:maxCount 1;
    shacl:name "versie";
    shacl:path owl:versionInfo
  ], [
    shacl:class skos:Concept;
    shacl:description "huidige toestand van gebruiksmogelijkheden";
    shacl:maxCount 1;
    shacl:name "statusVanGebruik";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#statusVanGebruik>
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Een trefwoord of tag die de resource beschrijft";
    shacl:maxCount 1;
    shacl:name "trefwoord";
    shacl:path dcat:keyword
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "landingspagina met informatie over de doelpopulatie";
    shacl:maxCount 1;
    shacl:name "doelpopulatie";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#doelpopulatie>
  ], [
    shacl:class skos:Concept;
    shacl:description "huidige toestand van de ontwikkelingsstatus ";
    shacl:maxCount 1;
    shacl:name "statusVanOntwikkeling";
    shacl:path <http://data.vlaanderen.be/ns/metadata-dcat#statusVanOntwikkeling>
  ];
  shacl:targetClass dcat:DataService .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DatasetShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <http://www.w3.org/ns/adms#Identifier>;
    shacl:description "Een unieke identificator van de dataset";
    shacl:maxCount "n"^^xsd:integer;
    shacl:minCount 1;
    shacl:name "identificator";
    shacl:path dc:identifier
  ], [
    shacl:datatype rdf:langString;
    shacl:description " Een bondige omschrijving van de dataset.";
    shacl:maxCount 1;
    shacl:name "beschrijving";
    shacl:path dc:description
  ], [
    <http://purl.org/linked-data/cube#codeList> <https://publications.europa.eu/en/web/eu-vocabularies/at-dataset/-/resource/dataset/access-right>;
    shacl:class skos:Concept;
    shacl:description "De toegankelijkheidscategorie van de dataset.";
    shacl:maxCount 1;
    shacl:name "toegankelijkheid";
    shacl:path dc:accessRights
  ], [
    shacl:class dcat:Distribution;
    shacl:description "een distributie van de de dataset.";
    shacl:name "distributie";
    shacl:path dcat:distribution
  ], [
    shacl:datatype xsd:anyURI;
    shacl:description "Een webpagina waar via een browser informatie kan gevonden worden over de dataset.";
    shacl:maxCount 1;
    shacl:name "landingspagina";
    shacl:path dcat:landingPage
  ], [
    shacl:datatype rdf:langString;
    shacl:description "De naam van de dataset";
    shacl:maxCount 1;
    shacl:name "titel";
    shacl:path dc:title
  ], [
    shacl:class schema:ContactPoint;
    shacl:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataset";
    shacl:maxCount 1;
    shacl:name "contactinformatie";
    shacl:path dcat:contactPoint
  ], [
    shacl:class skos:Concept;
    shacl:description "Het thema waartoe de dataset behoort";
    shacl:maxCount "n"^^xsd:integer;
    shacl:name "thema";
    shacl:path dcat:theme
  ];
  shacl:targetClass dcat:Dataset .

