    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "periodeVerzending";
    shacl:path <https://data.vlaanderen.be/ns/notificatie#periodeVerzending>
  ];
  shacl:targetClass schemas:InformAction .

