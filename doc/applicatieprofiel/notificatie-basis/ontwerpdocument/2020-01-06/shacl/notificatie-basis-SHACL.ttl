    shacl:path dc:relation
  ], [
    shacl:class schemas:Audience;
    shacl:description "De bestemmeling(en) die de informatie dien(t)(en) te ontvangen.";
    shacl:minCount 1;
    shacl:name "doelgroep";
    shacl:path schema:recipient
  ], [
    shacl:class skos:Concept;
    shacl:description "Het notificatiekanaal duidt het technisch medium aan waarlangs het Notificatiebericht wordt verzonden.";
    shacl:minCount 1;
    shacl:name "notificatiekanaal";
    shacl:path <https://data.vlaanderen.be/ns/notificatie#notificatiekanaal>
  ], [
    shacl:class <https://data.vlaanderen.be/ns/notificatie#Notificatiebericht>;
    shacl:description "De relatie tussen een Informeer Actie en het (de) Notificatiebericht(en) die hier uit voortkomen.";
    shacl:name "resulteertIn";
    shacl:path <https://data.vlaanderen.be/ns/notificatie#resulteertIn>
  ], [
    shacl:class schemas:PropertyValue;
    shacl:description "De inhoud van een Notificatiebericht.";
    shacl:name "inhoud";
    shacl:path <https://data.vlaanderen.be/ns/notificatie#inhoud>
  ], [
    shacl:class skos:Concept;
    shacl:description "Het onderwerp van de informeer actie.";
    shacl:minCount 1;
    shacl:name "onderwerp";
    shacl:path schemas:about
  ], [
    shacl:class <http://data.europa.eu/m8g/PublicOrganisation>;
    shacl:description "Duidt de agent aan die aanleiding gaf tot de InformeerActie.";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "agent";
    shacl:path schemas:agent
  ], [
    shacl:class <http://data.europa.eu/m8g/PeriodOfTime>;
