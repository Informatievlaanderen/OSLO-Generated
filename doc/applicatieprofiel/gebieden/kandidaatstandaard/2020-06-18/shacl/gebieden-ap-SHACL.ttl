<https://data.vlaanderen.be/shacl/gebieden-ap#GebiedShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdf:langString;
    shacl:description "Een naam voor het gebied.";
    shacl:name "naam";
    shacl:path dc:title
  ], [
    shacl:datatype xsd:dateTime;
    shacl:description "Datum waarop de huidige versie van het beheergebied in gebruik wordt genomen.";
    shacl:maxCount 1;
    shacl:name "begindatum";
    shacl:path <https://data.vlaanderen.be/ns/openbaardomein#begindatum>
  ], [
    shacl:class <http://www.opengis.net/ont/sf#Polygon>;
    shacl:description "De geometrie overeenkomstig met een bepaald gebied.";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "geometrie";
    shacl:path <http://www.w3.org/ns/locn#geometry>
  ], [
    shacl:datatype xsd:dateTime;
    shacl:description "Datum waarop de huidige versie van het beheergebied uit gebruik wordt genomen.";
    shacl:maxCount 1;
    shacl:name "einddatum";
    shacl:path <https://data.vlaanderen.be/ns/openbaardomein#einddatum>
  ];
  shacl:targetClass <https://data.vlaanderen.be/ns/openbaardomein#Gebied> .

