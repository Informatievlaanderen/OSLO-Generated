<https://data.vlaanderen.be/shacl/TIOD-IM#AanvragerShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <https://data.vlaanderen.be/ns/mobiliteit#Inname>;
    sh:description "Aangevraagde Inname.";
    sh:name "inname";
    sh:path <https://data.simcity.be/ns/tiod#inname>
  ];
  sh:targetClass <https://data.simcity.be/ns/tiod#Aanvrager> .

<https://data.vlaanderen.be/shacl/TIOD-IM#BeheerderShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://data.vlaanderen.be/ns/besluit#Vergunning>;
    sh:description "Beheerde Vergunning.";
    sh:name "vergunning";
    sh:path <https://data.simcity.be/ns/tiod#vergunning>
  ];
  sh:targetClass <https://data.simcity.be/ns/tiod#Beheerder> .

<https://data.vlaanderen.be/shacl/TIOD-IM#GeometrieShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdfs:Literal;
    sh:description "Geometrie uitgedrukt in wkt-formaat.";
    sh:maxCount 1;
    sh:name "wkt";
    sh:path <http://www.opengis.net/ont/geosparql#asWKT>
  ], [
    sh:datatype rdfs:Literal;
    sh:description "Geometrie uitgedrukt in gml-formaat.";
    sh:maxCount 1;
    sh:name "gml";
    sh:path <http://www.opengis.net/ont/geosparql#asGML>
  ];
  sh:targetClass <http://www.w3.org/ns/locn#Geometry> .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnameShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://data.europa.eu/m8g/PeriodOfTime>;
    sh:description "Wanneer de inname plaatsvindt.";
    sh:name "periode";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#periode>
  ], [
    sh:datatype xsd:boolean;
    sh:description "Duidt aan of de inname in een gebied met openbaar karakter doorgaat.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "openbaarDomein";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.openbaarDomein>
  ], [
    sh:class skos:Concept;
    sh:description "Type van inname";
    sh:minCount 1;
    sh:name "type";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.type>
  ], [
    sh:datatype rdf:langString;
    sh:description "Beschrijving over de inname.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:class <https://data.vlaanderen.be/ns/mobiliteit#Zone>;
    sh:description "Een bepaalde zone die deel uitmaakt van de inname.";
    sh:minCount 1;
    sh:name "zone";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#zone>
  ];
  sh:targetClass <https://data.vlaanderen.be/ns/mobiliteit#Inname> .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnametypeShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass skos:Concept .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnamevergunningShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <https://data.vlaanderen.be/ns/mobiliteit#Inname>;
    sh:description "De vergunde Inname.";
    sh:minCount 1;
    sh:name "vergunt";
    sh:path <https://data.simcity.be/ns/tiod#vergunt>
  ], [
    sh:class skos:Concept;
    sh:description "Type vergunning dat vergunt wordt.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "verguntType";
    sh:path <http://data.vlaanderen.be/ns/besluit#verguntType>
  ];
  sh:targetClass <http://data.vlaanderen.be/ns/besluit#Innamevergunning> .

<https://data.vlaanderen.be/shacl/TIOD-IM#PeriodeShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdfs:Literal;
    sh:description "Moment waarop de periode eindigt.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "einde";
    sh:path <http://data.europa.eu/m8g/endTime>
  ], [
    sh:datatype rdfs:Literal;
    sh:description "Moment waarop de periode begint.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "begin";
    sh:path <http://data.europa.eu/m8g/startTime>
  ];
  sh:targetClass <http://data.europa.eu/m8g/PeriodOfTime> .

<https://data.vlaanderen.be/shacl/TIOD-IM#VergunningShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class dc:Agent;
    sh:description "Wie de vergunning aanvraagt.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "aanvrager";
    sh:path <http://data.vlaanderen.be/ns/besluit#aanvrager>
  ], [
    sh:class <http://data.europa.eu/m8g/PeriodOfTime>;
    sh:description "TODO";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "tijdsinterval";
    sh:path dc:temporal
  ];
  sh:targetClass <http://data.vlaanderen.be/ns/besluit#Vergunning> .

<https://data.vlaanderen.be/shacl/TIOD-IM#ZoneShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class skos:Concept;
    sh:description "Soort zone.";
    sh:name "type";
    sh:path <https://data.vlaanderen.be/ns/mobiliteit#Zone.type>
  ], [
    sh:class <http://www.w3.org/ns/locn#Geometry>;
    sh:description "2D weergave van de zone.";
    sh:minCount 1;
    sh:name "geometrie";
    sh:path <http://www.w3.org/ns/locn#geometry>
  ];
  sh:targetClass <https://data.vlaanderen.be/ns/mobiliteit#Zone> .

<https://data.vlaanderen.be/shacl/TIOD-IM#ZonetypeShape> a sh:NodeShape;
  sh:closed false;
