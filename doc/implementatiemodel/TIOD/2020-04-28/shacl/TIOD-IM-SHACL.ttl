    shacl:class skos:Concept;
    shacl:description "Type vergunning dat vergunt wordt.";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "verguntType";
    shacl:path <http://data.vlaanderen.be/ns/besluit#verguntType>
  ];
  shacl:targetClass <http://data.vlaanderen.be/ns/besluit#Innamevergunning> .

<https://data.vlaanderen.be/shacl/TIOD-IM#PeriodeShape> a shacl:NodeShape;
