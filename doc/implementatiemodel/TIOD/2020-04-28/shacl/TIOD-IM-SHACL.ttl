<https://data.vlaanderen.be/shacl/TIOD-IM#ZoneShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class skos:Concept;
    shacl:description "Soort zone.";
    shacl:name "type";
    shacl:path <https://data.vlaanderen.be/ns/mobiliteit#Zone.type>
  ], [
