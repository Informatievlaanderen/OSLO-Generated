<https://data.vlaanderen.be/shacl/TIOD-IM#AanvragerShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <https://data.vlaanderen.be/ns/mobiliteit#Inname>;
    shacl:description "Aangevraagde Inname.";
    shacl:name "inname";
    shacl:path <https://data.simcity.be/ns/tiod#inname>
  ];
  shacl:targetClass <https://data.simcity.be/ns/tiod#Aanvrager> .

<https://data.vlaanderen.be/shacl/TIOD-IM#BeheerderShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <http://data.vlaanderen.be/ns/besluit#Vergunning>;
    shacl:description "Beheerde Vergunning.";
    shacl:name "vergunning";
    shacl:path <https://data.simcity.be/ns/tiod#vergunning>
  ];
  shacl:targetClass <https://data.simcity.be/ns/tiod#Beheerder> .

<https://data.vlaanderen.be/shacl/TIOD-IM#GeometrieShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype rdfs:Literal;
    shacl:description "Geometrie uitgedrukt in wkt-formaat.";
    shacl:maxCount 1;
    shacl:name "wkt";
    shacl:path <http://www.opengis.net/ont/geosparql#asWKT>
  ], [
    shacl:datatype rdfs:Literal;
    shacl:description "Geometrie uitgedrukt in gml-formaat.";
    shacl:maxCount 1;
    shacl:name "gml";
    shacl:path <http://www.opengis.net/ont/geosparql#asGML>
  ];
  shacl:targetClass <http://www.w3.org/ns/locn#Geometry> .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnameShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <http://data.europa.eu/m8g/PeriodOfTime>;
    shacl:description "Wanneer de inname plaatsvindt.";
    shacl:name "periode";
    shacl:path <https://data.vlaanderen.be/ns/mobiliteit#periode>
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Beschrijving over de inname.";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "beschrijving";
    shacl:path dc:description
  ], [
    shacl:class <https://data.vlaanderen.be/ns/mobiliteit#Zone>;
    shacl:description "Een bepaalde zone die deel uitmaakt van de inname.";
    shacl:minCount 1;
    shacl:name "zone";
    shacl:path <https://data.vlaanderen.be/ns/mobiliteit#zone>
  ], [
    shacl:datatype xsd:boolean;
    shacl:description "Duidt aan of de inname in een gebied met openbaar karakter doorgaat.";
    shacl:maxCount 1;
    shacl:minCount 1;
    shacl:name "openbaarDomein";
    shacl:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.openbaarDomein>
  ], [
    shacl:class skos:Concept;
    shacl:description "Type van inname";
    shacl:minCount 1;
    shacl:name "type";
    shacl:path <https://data.vlaanderen.be/ns/mobiliteit#Inname.type>
  ];
  shacl:targetClass <https://data.vlaanderen.be/ns/mobiliteit#Inname> .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnametypeShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:targetClass skos:Concept .

<https://data.vlaanderen.be/shacl/TIOD-IM#InnamevergunningShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:class <https://data.vlaanderen.be/ns/mobiliteit#Inname>;
