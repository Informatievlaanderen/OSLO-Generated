<https://data.vlaanderen.be/shacl/divrest-registrations-im#AdresvoorstellingShape> a shacl:NodeShape;
  shacl:closed false;
  shacl:property [
    shacl:datatype xsd:string;
    shacl:description "Alfanumerieke code officieel toegekend aan gebouweenheden, ligplaatsen, standplaatsen of percelen.";
    shacl:maxCount 1;
    shacl:name "huisnummer";
    shacl:path <https://data.vlaanderen.be/ns/adres#Adresvoorstelling.huisnummer>
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Gemeentenaam vh adres.";
    shacl:maxCount 1;
    shacl:name "gemeentenaam";
    shacl:path <https://data.vlaanderen.be/ns/adres#gemeentenaam>
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Straatnaam vh adres.";
    shacl:maxCount 1;
    shacl:name "straatnaam";
    shacl:path <http://www.w3.org/ns/locn#thoroughfare>
  ], [
    shacl:datatype xsd:string;
    shacl:description "Officieel toegekende alfanumerieke code die wordt toegevoegd aan het huisnummer om meerdere gebouweenheden, standplaatsen, ligplaatsen of percelen te onderscheiden die eenzelfde huisnummer hebben.";
    shacl:maxCount 1;
    shacl:name "busnummer";
    shacl:path <https://data.vlaanderen.be/ns/adres#Adresvoorstelling.busnummer>
  ], [
    shacl:datatype rdf:langString;
    shacl:description "Land waarin het adres gelegen is.";
    shacl:maxCount 1;
    shacl:name "land";
    shacl:path <https://data.vlaanderen.be/ns/adres#land>
  ], [
    shacl:datatype xsd:string;
    shacl:description "Code waarmee het geografisch gebied dat adressen voor postale doeleinden groepeert wordt aangeduid.";
    shacl:maxCount 1;
    shacl:name "postcode";
    shacl:path <http://www.w3.org/ns/locn#postCode>
  ];
  shacl:targetClass <http://www.w3.org/ns/locn#Address> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#AgentShape> a shacl:NodeShape;
