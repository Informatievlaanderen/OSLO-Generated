    shacl:name "adres";
    shacl:path <http://www.w3.org/ns/locn#address>
  ];
  shacl:targetClass schemas:ContactPoint .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#FormeleOrganisatieShape> a shacl:NodeShape;
