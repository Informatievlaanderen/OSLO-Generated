<https://data.vlaanderen.be/shacl/divrest-registrations-im#AdresvoorstellingShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype xsd:string;
    sh:description "Alfanumerieke code officieel toegekend aan gebouweenheden, ligplaatsen, standplaatsen of percelen.";
    sh:maxCount 1;
    sh:name "huisnummer";
    sh:path <https://data.vlaanderen.be/ns/adres#Adresvoorstelling.huisnummer>
  ], [
    sh:datatype xsd:string;
    sh:description "Officieel toegekende alfanumerieke code die wordt toegevoegd aan het huisnummer om meerdere gebouweenheden, standplaatsen, ligplaatsen of percelen te onderscheiden die eenzelfde huisnummer hebben.";
    sh:maxCount 1;
    sh:name "busnummer";
    sh:path <https://data.vlaanderen.be/ns/adres#Adresvoorstelling.busnummer>
  ], [
    sh:datatype rdf:langString;
    sh:description "Gemeentenaam vh adres.";
    sh:maxCount 1;
    sh:name "gemeentenaam";
    sh:path <https://data.vlaanderen.be/ns/adres#gemeentenaam>
  ], [
    sh:datatype xsd:string;
    sh:description "Code waarmee het geografisch gebied dat adressen voor postale doeleinden groepeert wordt aangeduid.";
    sh:maxCount 1;
    sh:name "postcode";
    sh:path <http://www.w3.org/ns/locn#postCode>
  ], [
    sh:datatype rdf:langString;
    sh:description "Straatnaam vh adres.";
    sh:maxCount 1;
    sh:name "straatnaam";
    sh:path <http://www.w3.org/ns/locn#thoroughfare>
  ], [
    sh:datatype rdf:langString;
    sh:description "Land waarin het adres gelegen is.";
    sh:maxCount 1;
    sh:name "land";
    sh:path <https://data.vlaanderen.be/ns/adres#land>
  ];
  sh:targetClass <http://www.w3.org/ns/locn#Address> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#AgentShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass dc:Agent .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#ContactInfoShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://www.w3.org/ns/locn#Address>;
    sh:description "Adres dat men kan aanschrijven of bezoeken.";
    sh:maxCount 1;
    sh:name "adres";
    sh:path <http://www.w3.org/ns/locn#address>
  ];
  sh:targetClass <https://schema.org/ContactPoint> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#FormeleOrganisatieShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass org:FormalOrganization .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#GeregistreerdPersoonShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Identificatiecode vd persoon ih register.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "registratie";
    sh:path <https://data.vlaanderen.be/ns/persoon#registratie>
  ];
  sh:targetClass <https://data.vlaanderen.be/ns/persoon#GeregistreerdPersoon> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#GeregistreerdeOrganisatieShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Identificator dat de organisatie verkreeg bij registratie.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "registratie";
    sh:path <http://www.w3.org/ns/regorg#registration>
  ], [
    sh:class skos:Concept;
    sh:description "Juridisch statuut van de geregistreerde organisatie.";
    sh:maxCount 1;
    sh:name "rechtsvorm";
    sh:path <https://data.vlaanderen.be/ns/organisatie#rechtsvorm>
  ];
  sh:targetClass <http://www.w3.org/ns/regorg#RegisteredOrganization> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#GetypeerdeStringShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass rdfs:Literal .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#IdentificatorShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdfs:Literal;
    sh:description "String gebruikt om het object uniek te identificeren. ";
    sh:maxCount 1;
    sh:name "identificator";
    sh:path skos:notation
  ];
  sh:targetClass <http://www.w3.org/ns/adms#Identifier> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#OrganisatieShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class skos:Concept;
    sh:description "Soort organisatie.";
    sh:name "type";
    sh:path <http://www.w3.org/ns/regorg#orgType>
  ], [
    sh:class <https://schema.org/ContactPoint>;
    sh:description "Informatie zoals email, telefoon... die toelaat de Organisatie te contacteren.";
    sh:name "contactinfo";
    sh:path schema:contactPoint
  ], [
    sh:datatype rdf:langString;
    sh:description "Naam waarmee de organisatie bij voorkeur wordt aangeduid.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "voorkeursnaam";
    sh:path skos:prefLabel
  ];
  sh:targetClass org:Organization .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#OrganisatieTypeShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass skos:Concept .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#PersoonShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <https://schema.org/ContactPoint>;
    sh:description "Informatie zoals email, telefoon... die toelaat de Persoon te contacteren.";
    sh:maxCount 1;
    sh:name "contactinfo";
    sh:path schema:contactPoint
  ], [
    sh:datatype xsd:string;
    sh:description "Naam die een kind bij geboorte wordt gegeven. Onderscheidt het kind van de andere kinderen in de familie.";
    sh:minCount 1;
    sh:name "voornaam";
    sh:path foaf:givenName
  ], [
    sh:datatype xsd:string;
    sh:description "Gedeelte van de volledige naam vd persoon ontvangen van de vorige generatie.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "achternaam";
    sh:path foaf:familyName
  ];
  sh:targetClass <http://www.w3.org/ns/person#Person> .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#RechtsvormtypeShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass skos:Concept .

<https://data.vlaanderen.be/shacl/divrest-registrations-im#TaalStringShape> a sh:NodeShape;
