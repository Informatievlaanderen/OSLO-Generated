      }
    },
    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/terreindeel#Boomplantplaats",
      "@type": "http://www.w3.org/2002/07/owl#Class",
      "description": {
        "nl": "De gereserveerde wortelzone, een ruimte (ondergronds) waar de wortels van bomen vrij kunnen groeien."
      },
      "name": {
        "nl": "Boomplantplaats"
      },
