logies:aantalSlaapplaatsen a owl:DatatypeProperty ;
    rdfs:label "aantal slaapplaatsen"@nl ;
    rdfs:comment "Totale aantal personen dat een logies of verhuureenheid kan bedienen."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range xsd:integer .

logies:aantalVerhuureenheden a owl:DatatypeProperty ;
    rdfs:label "aantal verhuureenheden"@nl ;
    rdfs:comment "Totale aantal eenheden waarover het logies beschikt."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range xsd:integer .

logies:behoortTotToeristischeRegio a owl:ObjectProperty ;
    rdfs:label "behoort tot toeristische regio"@nl ;
    rdfs:comment "Specifieert tot welke regio het logies behoort."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:ToeristischeRegio .

logies:bevatPlaats a owl:ObjectProperty ;
    rdfs:label "bevat plaats"@nl ;
    rdfs:comment "een plaats die zich fysiek in de regio. bevindt."@nl ;
    rdfs:domain logies:ToeristischeRegio ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range dcterms:Location .

logies:capaciteit a owl:ObjectProperty ;
    rdfs:label "capaciteit"@nl ;
    rdfs:comment "Een capaciteitsgegeven van de resource. De capaciteit is een gestructureerde waarde met een eenheid en een waarde."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range schema:QuantitativeValue .

logies:heeftAlternatieveUitbating a owl:ObjectProperty ;
    rdfs:label "heeft alternatieve uitbating"@nl ;
    rdfs:comment "De relatie tussen een logies en andere, commerciele uitbating van het logies. Bijvoorbeeld een B&B die ook als vakantiewoning wordt verhuurt."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Logies .

logies:heeftBeschrijving a owl:ObjectProperty ;
    rdfs:label "heeft beschrijving"@nl ;
    rdfs:comment "Beschrijving van het object."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Beschrijving .

logies:heeftKamer a owl:ObjectProperty ;
    rdfs:label "heeft kamer"@nl ;
    rdfs:comment "De relatie  tussen de kamer en de verhuureenheid die de kamer bevat."@nl ;
    rdfs:domain logies:Verhuureenheid ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Ruimte ;
    rdfs:subPropertyOf schema:containsPlace .

logies:heeftKwaliteitsLabel a owl:ObjectProperty ;
    rdfs:label "heeft kwaliteitslabel"@nl ;
    rdfs:comment "Een kwaliteitslabel dat is toegekend aan het logies. "@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Kwaliteitslabel .

logies:heeftMedia a owl:ObjectProperty ;
    rdfs:label "heeft media"@nl ;
    rdfs:comment "Media gerelateerd aan het object."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:MediaObject .

logies:heeftRegistratie a owl:ObjectProperty ;
    rdfs:label "heeft registratie"@nl ;
    rdfs:comment "De registratie van het logies bij Toerisme Vlaanderen."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Registratie .

logies:heeftVerhuureenheid a owl:ObjectProperty ;
    rdfs:label "heeft verhuureenheid"@nl ;
    rdfs:comment "Een verhuureenheid die gerelateerd is aan een logies."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Verhuureenheid .

logies:isSpecialisatieVan a owl:ObjectProperty ;
    rdfs:label "is specialisatie van"@nl ;
    rdfs:comment "Verwijst naar de bronfaciliteit waarvan deze faciliteit een specialisatie is."@nl ;
    rdfs:domain logies:Faciliteit ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range logies:Faciliteit ;
    rdfs:subPropertyOf skos:broader .

logies:locatie a owl:ObjectProperty ;
    rdfs:label "locatie"@nl ;
    rdfs:comment "De geometrische locatie van het object."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range locn:Geometry ;
    rdfs:subPropertyOf locn:geometry .

logies:locatieaanduiding a rdf:Property ;
    rdfs:label "locatie aanduiding"@nl ;
    rdfs:comment "Locatie waar de ruimte zich bevind, bijvoorbeeld het verdiep."@nl ;
    rdfs:domain logies:Ruimte ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> .

logies:onthaalAdres a owl:ObjectProperty ;
    rdfs:label "onthaal adres"@nl ;
    rdfs:comment "Adres waar logies zich kunnen aanmelden."@nl ;
    rdfs:domain logies:Logies ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range locn:Address .

logies:onthaalLocatie a owl:ObjectProperty ;
    rdfs:label "onthaal locatie"@nl ;
    rdfs:comment "De geometrische locatie van het object."@nl ;
    rdfs:domain rdf:Resource ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range locn:Geometry ;
    rdfs:subPropertyOf locn:geometry .

logies:registratieStatus a owl:ObjectProperty ;
    rdfs:label "registratie status"@nl ;
    rdfs:comment "Status van het geregistreerde logies."@nl ;
    rdfs:domain logies:Registratie ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:range skos:Concept .

logies:Beschrijving a owl:Class ;
    rdfs:label "Beschrijving"@nl ;
    rdfs:comment "Een omschrijving van een object."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:CreativeWork .

logies:Kwaliteitslabel a owl:Class ;
    rdfs:label "Kwaliteitslabel"@nl ;
    rdfs:comment "Een label dat wordt toegekend aan een entiteit wanneer deze aan bepaalde voorwaarden voldoen."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> .

logies:MediaObject a owl:Class ;
    rdfs:label "Media Object"@nl ;
    rdfs:comment "Een media-object, zoals bijvoorbeeld een afbeelding, video of audio-object."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:CreativeWork,
        schema:MediaObject .

locn:geometry a rdf:Property ;
    rdfs:label "geometrie"@nl .

logies:Faciliteit a owl:Class ;
    rdfs:label "Faciliteit"@nl ;
    rdfs:comment "Aanwezige infrastructuur of dienst."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:LocationFeatureSpecification .

logies:Registratie a owl:Class ;
    rdfs:label "Registratie"@nl ;
    rdfs:comment "Registratie van het logies bij een officiële instantie."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf <https://data.vlaanderen.be/ns/generiek#Versie> .

logies:Ruimte a owl:Class ;
    rdfs:label "Ruimte"@nl ;
    rdfs:comment "Een ruimtelijk onderdeel binnen een verhuureenheid, meestal gescheiden van andere ruimtes door binnenmuren. Bijvoorbeeld een terrein, een kamer, enz."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:Accomodation .

logies:ToeristischeRegio a owl:Class ;
    rdfs:label "Toeristische regio"@nl ;
    rdfs:comment "Geografische afbakening van een promotioneel samenhangend gebied. "@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf dcterms:Location .

logies:Verhuureenheid a owl:Class ;
    rdfs:label "Verhuureenheid"@nl ;
    rdfs:comment "Een afzonderlijk te huren kamer, ruimte, oppervlakte of onderdeel waar kan worden overnacht."@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:Accomodation .

logies:Logies a owl:Class ;
    rdfs:label "Logies"@nl ;
    vann:usageNote "Zie ook https://schema.org/docs/hotels.html"@nl ;
    rdfs:comment """Elke constructie, inrichting, ruimte of terrein, in eender welke?\r
\r
vorm, dat aan één of meer toeristen tegen betaling de mogelijkheid tot verblijf biedt voor een of meer nachten, en dat wordt aangeboden op de toeristische markt."""@nl ;
    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/logies> ;
    rdfs:subClassOf schema:LodgingBusiness .

