<https://data.vlaanderen.be/ns/besluitvorming#genereertVerslag> a owl:ObjectProperty;
  rdfs:label "genereert verslag"@nl;
  rdfs:comment "de schriftelijke neerslag van (een onderdeel) van deze behandeling van agendapunt."@nl;
  rdfs:domain <http://data.vlaanderen.be/ns/besluit#BehandelingVanAgendapunt>;
