  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range <https://data.vlaanderen.be/ns/bestuur#Bestuursorgaan>;
  rdfs:subPropertyOf prov:wasStartedBy .

<https://data.vlaanderen.be/ns/besluitvorming#korteTitel> a owl:DatatypeProperty;
  rdfs:label "korte titel"@nl;
  rdfs:comment "Een verkorte vorm van de naam."@nl;
  rdfs:domain <http://data.vlaanderen.be/ns/besluit#Agendapunt>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range xsd:string;
  rdfs:subPropertyOf dc:alternative .

<https://data.vlaanderen.be/ns/besluitvorming#resultaat> a owl:ObjectProperty;
  rdfs:label "resultaat"@nl;
  <http://purl.org/vocab/vann/usageNote> "bijvoorbeeld goedkeuring, goedkeuring mits voorwaarden,afkeuring, verdaging,..."@nl;
  rdfs:comment "Eindoordeel van de beoordeling."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/besluitvorming#Beslissingsactiviteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range skos:Concept .

<https://data.vlaanderen.be/ns/besluitvorming#status> a rdf:Property;
  rdfs:label "status"@nl;
  <http://purl.org/vocab/vann/usageNote> "Bijvoorbeeld Ontwerp, Tussentijds goedgekeurd, Definitief, etc."@nl;
  rdfs:comment "De toestand waarin de agenda zich bevindt voor een specifiek type."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/besluitvorming#Agenda>;
