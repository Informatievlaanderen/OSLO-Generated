  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range <https://data.vlaanderen.be/ns/besluitvorming#Beslissingsactiviteit> .

<https://data.vlaanderen.be/ns/besluitvorming#heeftNotulen> a owl:ObjectProperty;
  rdfs:label "heeft notulen"@nl;
  rdfs:comment "De formele notulen van de vergadering."@nl;
  rdfs:domain <http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range <https://data.vlaanderen.be/ns/dossier#Stuk> .

<https://data.vlaanderen.be/ns/besluitvorming#isAgendaVoor> a owl:ObjectProperty;
  rdfs:label "is agenda voor"@nl;
  rdfs:comment "De Vergaderactiviteit waarvoor de Agenda wordt voorbereid."@nl;
  rdfs:domain <https://data.vlaanderen.be/ns/besluitvorming#Agenda>;
  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range <http://data.vlaanderen.be/ns/besluit#Vergaderactiviteit> .

<https://data.vlaanderen.be/ns/besluitvorming#isGehoudenDoor> a owl:ObjectProperty;
  rdfs:label "is gehouden door"@nl;
  rdfs:comment "Duidt aan door welk orgaan de vergadering is gehouden."@nl;
