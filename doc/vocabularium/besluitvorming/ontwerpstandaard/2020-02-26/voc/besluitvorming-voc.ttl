  rdfs:isDefinedBy <https://data.vlaanderen.be/ns/besluitvorming>;
  rdfs:range <https://data.vlaanderen.be/ns/besluitvorming#Aanleiding> .

<https://data.vlaanderen.be/ns/besluitvorming#heeftBeslissing> a owl:ObjectProperty;
  rdfs:label "heeft beslissing"@nl;
  rdfs:comment "De Beslissing die volgt uit de Behandeling van het Agendapunt."@nl;
