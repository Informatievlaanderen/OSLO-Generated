@prefix dc: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL> <https://data.vlaanderen.be/shacl/DCAT-AP-VLshapes> <https://data.vlaanderen.be/shacl/DCAT-AP-VL#DatasetShape>,
    <https://data.vlaanderen.be/shacl/DCAT-AP-VL#LicentieDocumentShape>,
    <https://data.vlaanderen.be/shacl/DCAT-AP-VL#DistributieShape>,
    <https://data.vlaanderen.be/shacl/DCAT-AP-VL#AgentShape>,
    <https://data.vlaanderen.be/shacl/DCAT-AP-VL#DatasetCatalogusShape> .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL#AgentShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdf:langString;
    sh:description "De naam van de uitgever";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "naam";
    sh:path foaf:name
  ];
  sh:targetClass foaf:Agent .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL#DatasetCatalogusShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class foaf:Agent;
    sh:description "De uitgever van de datasetcatalogus";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeftUitgever";
    sh:path dc:publisher
  ], [
    sh:class dcat:Dataset;
    sh:description "Een dataset die behoort tot de datasetcatalogus";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "heeftDataset";
    sh:path dcat:dataset
  ], [
    sh:class dc:LicenseDocument;
    sh:description "De licentie van de datasetcatalogus.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeftLicentie";
    sh:path dc:license
  ], [
    sh:datatype rdf:langString;
    sh:description "Een bondige omschrijving van de datasetcatalogus. ";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:datatype rdf:langString;
    sh:description "De naam van de datasetcatalogus.";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "titel";
    sh:path dc:title
  ];
  sh:targetClass dcat:Catalog .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL#DatasetShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdf:langString;
    sh:description "Een bondige omschrijving van de dataset.";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    <http://purl.org/linked-data/cube#codeList> <https://publications.europa.eu/en/web/eu-vocabularies/at-dataset/-/resource/dataset/access-right>;
    sh:class skos:Concept;
    sh:description "De toegankelijkheidscategorie van de dataset.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "toegankelijkheid";
    sh:path dc:accessRights
  ], [
    <http://purl.org/linked-data/cube#codeList> <https://vocab.belgif.be/auth/datatheme>;
    sh:class skos:Concept;
    sh:description "Het thema waartoe de dataset behoort.";
    sh:maxCount "n"^^xsd:integer;
    sh:name "thema";
    sh:path dcat:theme
  ], [
    sh:datatype rdf:langString;
    sh:description "De naam van de dataset";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    sh:class dcat:Distribution;
    sh:description "De distributie van een dataset.";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "heeftDistributie";
    sh:path dcat:distribution
  ], [
    sh:class vcard:Kind;
    sh:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataset. ";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "contactinfo";
    sh:path dcat:contactPoint
  ], [
    sh:class foaf:Agent;
    sh:description "De uitgever van de dataset. ";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeftUitgever";
    sh:path dc:publisher
  ];
  sh:targetClass dcat:Dataset .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL#DistributieShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdf:langString;
    sh:description "Een naam voor de distributie";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    sh:datatype xsd:anyURI;
    sh:description "De URL waar de data kan gevonden worden.";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "toegangsURL";
    sh:path dcat:accessURL
  ], [
    sh:class dc:LicenseDocument;
    sh:description "De licentie van de distributie";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "heeftLicentie";
    sh:path dc:license
  ];
  sh:targetClass dcat:Distribution .

<https://data.vlaanderen.be/shacl/DCAT-AP-VL#LicentieDocumentShape> a sh:NodeShape;
  sh:closed false;
  sh:targetClass dc:LicenseDocument .

 [
    sh:datatype xsd:anyURI;
    sh:description "Het e-mail adres waarmee je contact kan opnemen";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "e-mail";
    sh:path vcard:hasEmail
  ] .
