@prefix dc: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap> <https://data.vlaanderen.be/shacl/metadata-voor-services-apshapes> <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#AgentShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusRecordShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DistributieShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DatasetShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DataServiceShape>,
    <https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusResourceShape> .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#AgentShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype rdf:langString;
    sh:description "De naam van de agent";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "naam";
    sh:path <http://data.vlaanderen.be/ns/generiek#naam>
  ];
  sh:targetClass dc:Agent .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusRecordShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class dcat:Resource;
    sh:description "de geregistreerde catalogusResource";
    sh:maxCount 1;
    sh:name "hoofdonderwerp";
    sh:path foaf:primaryTopic
  ], [
    sh:datatype rdf:langString;
    sh:description "de naam van het registratie van de catalogusresource ";
    sh:maxCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "een identificator van het catalogusrecord";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "identificator";
    sh:path dc:identifier
  ], [
    sh:datatype xsd:dateTime;
    sh:description "Het moment waarop de registratie van de catalogusResource voor de eerste keer gebeurde in de catalogus";
    sh:maxCount 1;
    sh:name "aanmaakdatum";
    sh:path dc:issued
  ];
  sh:targetClass dcat:CatalogRecord .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusResourceShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class dc:Agent;
    sh:description "De uitgever van de catalogusResource";
    sh:maxCount 1;
    sh:name "uitgever";
    sh:path dc:publisher
  ];
  sh:targetClass dcat:Resource .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#CatalogusShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class dcat:CatalogRecord;
    sh:description "provenance informatie over de registratie van een catalogsResource in de catalogus ";
    sh:name "record";
    sh:path dcat:record
  ], [
    sh:datatype rdf:langString;
    sh:description "een bondige omschrijving van de catalogus";
    sh:maxCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:class dc:LicenseDocument;
    sh:description "de licentie voor de catalogus";
    sh:maxCount 1;
    sh:name "licentie";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#licentie>
  ], [
    sh:class dcat:DataService;
    sh:description "geregistreerde DataServices in de catalogus";
    sh:name "heeftDataService";
    sh:path dcat:service
  ], [
    sh:class dcat:Dataset;
    sh:description "geregistreerde dataset in de catalogus ";
    sh:name "heeftDataset";
    sh:path dcat:dataset
  ], [
    sh:class vcard:Kind;
    sh:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de catalogus";
    sh:maxCount "n"^^xsd:integer;
    sh:name "contactinformatie";
    sh:path dcat:contactPoint
  ], [
    sh:class dc:Agent;
    sh:description "de uitgever van een catalogus";
    sh:maxCount 1;
    sh:name "uitgever";
    sh:path dc:publisher
  ], [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "een identificator voor de catalogus";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "identificator";
    sh:path dc:identifier
  ], [
    sh:datatype rdf:langString;
    sh:description "de naam voor de catalogus";
    sh:maxCount 1;
    sh:name "titel";
    sh:path dc:title
  ];
  sh:targetClass dcat:Catalog .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DataServiceShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:datatype xsd:anyURI;
    sh:description "Algemene pagina met vaak beschrijvende tekst";
    sh:maxCount 1;
    sh:name "landingspagina";
    sh:path dcat:landingPage
  ], [
    sh:datatype xsd:anyURI;
    sh:description "Een beschrijving van de diensten die beschikbaar zijn via de end-points, met inbegrip van hun werking, parameters, enz. ";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "endpointbeschrijving";
    sh:path dcat:endpointDescription
  ], [
    sh:datatype xsd:anyURI;
    sh:description "De rootlocatie of het primaire eindpunt van de dienst (een web-resolvable URI)";
    sh:maxCount 1;
    sh:name "endpointURL";
    sh:path dcat:endpointURL
  ], [
    sh:datatype xsd:anyURI;
    sh:description "landingspagina met informatie over de genomen voorwaarden om de service te mogen gebruiken. ";
    sh:maxCount 1;
    sh:name "gebruiksbepaling";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#gebruiksbepaling>
  ], [
    sh:datatype xsd:anyURI;
    sh:description "landingspagina met informatie over de doelpopulatie";
    sh:maxCount 1;
    sh:name "doelpopulatie";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#doelpopulatie>
  ], [
    sh:class vcard:Kind;
    sh:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataservice. ";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "contactinformatie";
    sh:path dcat:contactPoint
  ], [
    sh:datatype xsd:anyURI;
    sh:description "landingspagina met informatie over kwaliteit van de dienstverlening";
    sh:maxCount 1;
    sh:name "dienstverleningsKwaliteit";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#dienstverleningsKwaliteit>
  ], [
    sh:datatype xsd:anyURI;
    sh:description "landingspagina met informatie over authenticatie";
    sh:maxCount 1;
    sh:name "authenticatie";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#authenticatie>
  ], [
    sh:datatype rdf:langString;
    sh:description "naam voor de dataService. ";
    sh:maxCount 1;
    sh:minCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Een unieke identificator van de dataService";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "identificator";
    sh:path dc:identifier
  ], [
    sh:datatype rdf:langString;
    sh:description "Een bondige omschrijving van de dataService";
    sh:maxCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:class skos:Concept;
    sh:description "huidige toestand van de ontwikkelingsstatus ";
    sh:maxCount 1;
    sh:name "statusVanOntwikkeling";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#statusVanOntwikkeling>
  ], [
    sh:datatype xsd:string;
    sh:description "unieke aanduiding van de gedeployde service toestand gegeven door de endpointbeheerder";
    sh:maxCount 1;
    sh:name "versie";
    sh:path owl:versionInfo
  ], [
    sh:class skos:Concept;
    sh:description "huidige toestand van gebruiksmogelijkheden";
    sh:maxCount 1;
    sh:name "statusVanGebruik";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#statusVanGebruik>
  ], [
    sh:datatype rdf:langString;
    sh:description "Een trefwoord of tag die de resource beschrijft";
    sh:maxCount 1;
    sh:name "trefwoord";
    sh:path dcat:keyword
  ], [
    sh:class dcat:Dataset;
    sh:description "data die aangeboden wordt door de datasService";
    sh:name "biedtInformatieAanOver";
    sh:path dcat:servesdataset
  ], [
    sh:datatype xsd:anyURI;
    sh:description "landingspagina met informatie over de wettelijke beperkingen i.v.m. dataService";
    sh:maxCount 1;
    sh:name "wettelijkeBeperkingen";
    sh:path <http://data.vlaanderen.be/ns/metadata-dcat#wettelijkeBeperkingen>
  ];
  sh:targetClass dcat:DataService .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DatasetShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class skos:Concept;
    sh:description "Het thema waartoe de dataset behoort";
    sh:maxCount "n"^^xsd:integer;
    sh:name "thema";
    sh:path dcat:theme
  ], [
    sh:class dcat:Distribution;
    sh:description "een distributie van de de dataset.";
    sh:name "distributie";
    sh:path dcat:distribution
  ], [
    sh:datatype rdf:langString;
    sh:description "De naam van de dataset";
    sh:maxCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    <http://purl.org/linked-data/cube#codeList> <https://publications.europa.eu/en/web/eu-vocabularies/at-dataset/-/resource/dataset/access-right>;
    sh:class skos:Concept;
    sh:description "De toegankelijkheidscategorie van de dataset.";
    sh:maxCount 1;
    sh:name "toegankelijkheid";
    sh:path dc:accessRights
  ], [
    sh:class schema:ContactPoint;
    sh:description "De contactinformatie via men in contact kan treden met de verantwoordelijken van de dataset";
    sh:maxCount 1;
    sh:name "contactinformatie";
    sh:path dcat:contactPoint
  ], [
    sh:datatype xsd:anyURI;
    sh:description "Een webpagina waar via een browser informatie kan gevonden worden over de dataset.";
    sh:maxCount 1;
    sh:name "landingspagina";
    sh:path dcat:landingPage
  ], [
    sh:datatype rdf:langString;
    sh:description " Een bondige omschrijving van de dataset.";
    sh:maxCount 1;
    sh:name "beschrijving";
    sh:path dc:description
  ], [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Een unieke identificator van de dataset";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "identificator";
    sh:path dc:identifier
  ];
  sh:targetClass dcat:Dataset .

<https://data.vlaanderen.be/shacl/metadata-voor-services-ap#DistributieShape> a sh:NodeShape;
  sh:closed false;
  sh:property [
    sh:class <http://www.w3.org/ns/adms#Identifier>;
    sh:description "Een unieke identificator van de distributie";
    sh:maxCount "n"^^xsd:integer;
    sh:minCount 1;
    sh:name "identificator";
    sh:path dc:identifier
  ], [
    sh:datatype xsd:anyURI;
    sh:description "De URL waar de data gedownload kan worden. ";
    sh:maxCount 1;
    sh:name "downloadURL";
    sh:path dcat:downloadURL
  ], [
    sh:class dcat:DataService;
    sh:description "de dataservice die deze distributie aanbiedt";
    sh:name "wordtAangebodenDoor";
    sh:path dcat:accessService
  ], [
    sh:class dc:LicenseDocument;
    sh:description "de licentie van de distributie";
    sh:maxCount 1;
    sh:name "licentie";
    sh:path dc:license
  ], [
    sh:datatype rdf:langString;
    sh:description "naam van de distributie";
    sh:maxCount 1;
    sh:name "titel";
    sh:path dc:title
  ], [
    sh:datatype xsd:anyURI;
    sh:description "De URL waar de data kan gevonden worden.";
    sh:maxCount 1;
    sh:name "toegangsURL";
    sh:path dcat:accessURL
  ];
  sh:targetClass dcat:Distribution .

 [
    sh:datatype xsd:anyURI;
    sh:description "Het e-mail adres waarmee je contact kan opnemen.";
    sh:maxCount 1;
