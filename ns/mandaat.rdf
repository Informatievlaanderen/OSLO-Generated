<rdfs:comment xml:lang='nl'>Het type van de lijst.</rdfs:comment>

<rdfs:domain rdf:resource='http://data.vlaanderen.be/ns/mandaat#Kandidatenlijst' />

<rdfs:isDefinedBy rdf:resource='http://data.vlaanderen.be/ns/mandaat' />

<rdfs:range rdf:resource='http://www.w3.org/2004/02/skos/core#Concept' />

</owl:ObjectProperty>

