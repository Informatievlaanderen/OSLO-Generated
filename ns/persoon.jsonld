      "rdfs:label": {
        "@value": "Gezin",
        "@language": "nl"
      },
      "http://purl.org/vocab/vann/usageNote": {
        "@value": "Ruimer begrip dan familie waar het gezin louter bestaat uit nabije bloedverwanten, bv een eenpersoonsgezin of living apart together. Gezinsleden hebben doorgaans dezelfde verblijfplaats.",
        "@language": "nl"
      }
