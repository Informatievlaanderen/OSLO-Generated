    {
      "@id": "https://data.vlaanderen.be/ns/openbaardomein/begroeidvoorkomen#Houtachtigen",
      "rdfs:label": {
        "@value": "Houtachtigen",
        "@language": "nl"
      },
      "rdfs:comment": {
        "@value": "Houtige planten of houtige gewassen (planta lignosa) zijn overblijvende planten die worden gekenmerkt door secundaire diktegroei, waardoor de takken, stammen en wortels veel hout bevatten.",
        "@language": "nl"
      },
      "@type": "owl:Class",
      "rdfs:isDefinedBy": {
